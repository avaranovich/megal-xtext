<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" name="MegaL" version="8.1.0">
  <ownedViewpoints name="MegalInstance" modelFileExtension="megal">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="MegalModel" domainClass="megaL.MegaLDefinition">
      <defaultLayer name="Default">
        <nodeMappings xsi:type="description_1:NodeMappingImport" name="Entity" labelDirectEdit="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@toolSections.0/@ownedTools[name='setEntityName']" semanticCandidatesExpression="[eds/]" domainClass="megaL.ED">
          <style xsi:type="style:SquareDescription" labelSize="10" showIcon="false" labelExpression="[name + ':' + self.getTypeName()/]" tooltipExpression="[self.getMessage()/]" labelPosition="node" resizeKind="NSEW" width="13" height="7" color="//@userColorsPalettes[name='MegalColors']/@entries[name='EntityTypeColor']">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="Relation" semanticCandidatesExpression="[rds/]" sourceMapping="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@nodeMappings[name='Entity']" targetMapping="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@nodeMappings[name='Entity']" targetFinderExpression="[target/]" sourceFinderExpression="[source/]" domainClass="megaL.RD" useDomainElement="true">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="10" showIcon="false" labelExpression="[rel.name/]">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <toolSections name="MegalInstanceTools" label="MagaL Instance Tools">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="createEntity" label="Entity" nodeMappings="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@nodeMappings[name='Entity']" iconPath="org.softlang.megal.sirius/icons/entity.gif">
            <variable name="container">
              <subVariables xsi:type="tool_1:SelectModelElementVariable" name="selectedType" candidatesExpression="[container.oclAsType(MegaLDefinition).effectiveETDs()/]" message="Select type of entity"/>
            </variable>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="megaL.ED" referenceName="eds">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="newEntity">
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="megaL.UseETDRef" referenceName="type">
                      <subModelOperations xsi:type="tool_1:SetValue" featureName="ref" valueExpression="[selectedType/]"/>
                    </subModelOperations>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="createRelation" label="Relation" edgeMappings="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@edgeMappings[name='Relation']" iconPath="org.softlang.megal.sirius/icons/relation.gif">
            <sourceVariable name="from"/>
            <targetVariable name="to">
              <subVariables xsi:type="tool_1:SelectModelElementVariable" name="selectedType" candidatesExpression="[from.eContainer().oclAsType(MegaLDefinition).effectiveRTDs()/]" message="Select type of relation"/>
            </targetVariable>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="[from.eContainer()/]">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="megaL.RD" referenceName="rds">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="rel" valueExpression="[selectedType/]"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="source" valueExpression="[from/]"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="target" valueExpression="[to/]"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:DirectEditLabel" name="setEntityName" inputLabelExpression="[name/]">
            <mask mask="{0}"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="[arg0/]"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:SelectionWizardDescription" name="typer" label="Typer" precondition="[self.oclIsTypeOf(RD)._or(self.oclIsTypeOf(ED))/]" candidatesExpression="[if self.oclIsTypeOf(ED) then self.eContainer().oclAsType(MegaLDefinition).effectiveETDs() else self.eContainer().oclAsType(MegaLDefinition).effectiveRTDs() endif/]" iconPath="org.softlang.megal.sirius/icons/typer.gif" windowTitle="Selection entity type">
            <element name="selectedType"/>
            <containerView name="containerView"/>
            <container name="container"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="[container/]">
                <subModelOperations xsi:type="tool_1:If" conditionExpression="[self.oclIsTypeOf(RD)/]">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="rel" valueExpression="[selectedType/]"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <customization>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="[self.oclIsTypeOf(RD)/]">
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@edgeMappings[name='Relation']/@style" attributeName="lineStyle" value="[self.oclAsType(RD).getLineStyle()/]"/>
          </vsmElementCustomizations>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="[self.oclIsTypeOf(ED)._and(self.oclAsType(ED).hasErrorMessage())/]">
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@nodeMappings[name='Entity']/@style" referenceName="borderColor">
              <value href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
            </featureCustomizations>
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@nodeMappings[name='Entity']/@style" attributeName="borderSizeComputationExpression" value="[2/]"/>
          </vsmElementCustomizations>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="[self.oclIsTypeOf(ED)._and(self.oclAsType(ED).hasWarningMessage())/]">
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@nodeMappings[name='Entity']/@style" referenceName="borderColor">
              <value href="environment:/viewpoint#//@systemColors/@entries[name='yellow']"/>
            </featureCustomizations>
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@nodeMappings[name='Entity']/@style" attributeName="borderSizeComputationExpression" value="[2/]"/>
          </vsmElementCustomizations>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="[self.oclIsTypeOf(RD)._and(self.oclAsType(RD).hasErrorMessage())/]">
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@edgeMappings[name='Relation']/@style" referenceName="strokeColor">
              <value href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
            </featureCustomizations>
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@edgeMappings[name='Relation']/@style" attributeName="sizeComputationExpression" value="[3/]"/>
          </vsmElementCustomizations>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="[self.oclIsTypeOf(RD)._and(self.oclAsType(RD).hasWarningMessage())/]">
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@edgeMappings[name='Relation']/@style" referenceName="strokeColor">
              <value href="environment:/viewpoint#//@systemColors/@entries[name='yellow']"/>
            </featureCustomizations>
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='MegalInstance']/@ownedRepresentations[name='MegalModel']/@defaultLayer/@edgeMappings[name='Relation']/@style" attributeName="sizeComputationExpression" value="[3/]"/>
          </vsmElementCustomizations>
        </customization>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="org.softlang.megal.sirius.EObjectServices"/>
  </ownedViewpoints>
  <userColorsPalettes name="MegalColors">
    <entries xsi:type="description:ComputedColor" name="EntityTypeColor" red="[self.oclAsType(ED).getEntityRed()/]" green="[self.oclAsType(ED).getEntityGreen()/]" blue="[self.oclAsType(ED).getEntityBlue()/]"/>
  </userColorsPalettes>
</description:Group>
